using numtodelay:= (if Digit_index>3 then -ceil((digit_index-4)/4) else 0) Do~
using lastchar:= (Digits+Max_(0,ceil((Digits-3)/3))) Do~
using delayarray:= (if Digit_index<=lastchar then delay_array_generic_(Gettens,numtodelay,Digit_index) else 0) Do~
using insert_commas:= (if digit_index>lastchar then '' else if digit_index>3 and mod(digit_index,4)=0 and digit_index<>lastchar then ',' else delayarray) Do~
if datanode=0 then 0 else ~
if isnum then Join(insert_commas,Digit_index) else datanode
